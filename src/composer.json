{
    "name": "xdrew/monadungeon",
    "license": "proprietary",
    "type": "project",
    "version": "0.0.1",
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "amphp/amp": "^3",
        "baldinof/roadrunner-bundle": "^3.1.2",
        "bunny/bunny": "^0.5.5",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.1",
        "lcobucci/jwt": "^5.2",
        "psr/clock": "^1.0",
        "ramsey/uuid": "^4.7",
        "react/async": "^4.2",
        "react/event-loop": "^1.5",
        "react/promise": "^2.11",
        "revolt/event-loop-adapter-react": "^1.1",
        "symfony/console": "~7.0.0",
        "symfony/dotenv": "~7.0.0",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "~7.0.0",
        "symfony/monolog-bundle": "^3.10",
        "symfony/runtime": "~7.0.0",
        "symfony/security-bundle": "~7.0.0",
        "symfony/serializer": "~7.0.0",
        "symfony/yaml": "~7.0.0",
        "telephantast/bunny-transport": "main-dev",
        "telephantast/doctrine-persistence": "^0.1@dev",
        "telephantast/message": "^0.1@dev",
        "telephantast/message-bus": "^0.1@dev",
        "telephantast/telephantast-bundle": "^0.1@dev",
        "telephantast/thesis-amqp-transport": "^0.1@dev",
        "vaimo/composer-patches": "^5.1",
        "web3p/ethereum-tx": "^0.4.3",
        "web3p/web3.php": "^0.1.6"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "ergebnis/composer-normalize": "^2.42",
        "friendsofphp/php-cs-fixer": "^3.51",
        "icanhazstring/composer-unused": "^0.8.11",
        "maglnet/composer-require-checker": "^4.10",
        "phpunit/phpunit": "^10",
        "phpyh/coding-standard": "^2.6",
        "phpyh/service-dumper-bundle": "^0.3.4",
        "rector/rector": "^1.0"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/Infrastructure/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [
            "tests/Infrastructure/MessageBus/functions.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "vaimo/composer-patches": true
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": false,
            "forward-command": true
        },
        "patcher": {
            "search": "patches"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "7.0.*"
        }
    }
}
